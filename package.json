{
  "name": "discord-irc",
  "version": "0.4.1",
  "description": "Connects IRC and Discord channels by sending messages back and forth.",
  "keywords": [
    "discord",
    "irc",
    "gateway",
    "bot",
    "discord-irc",
    "reactiflux"
  ],
  "engines": {
    "node": ">=0.12.0"
  },
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:reactiflux/discord-irc.git"
  },
  "bugs": {
    "url": "https://github.com/reactiflux/discord-irc/issues"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "babel lib --out-dir dist",
    "lint": "eslint . --ignore-path .gitignore",
    "coverage": "istanbul cover _mocha -- --compilers js:babel/register $(find test -name '*.test.js') && istanbul report cobertura",
    "test": "npm run lint && npm run coverage"
  },
  "author": {
    "name": "Martin Ek <mail@ekmartin.no>"
  },
  "license": "MIT",
  "dependencies": {
    "check-env": "~1.2.0",
    "commander": "~2.9.0",
    "discord.js": "~5.1.0",
    "irc": "~0.4.0",
    "lodash": "~3.10.1",
    "strip-json-comments": "~2.0.0",
    "winston": "~2.1.1"
  },
  "devDependencies": {
    "babel": "~5.8.29",
    "babel-eslint": "~4.1.3",
    "chai": "~3.4.0",
    "eslint": "~1.10.3",
    "eslint-config-airbnb": "~2.1.1",
    "istanbul": "~0.4.0",
    "mocha": "~2.3.3",
    "sinon": "~1.17.2",
    "sinon-chai": "~2.8.0"
  }
}
